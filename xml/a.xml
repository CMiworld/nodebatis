<?xml version='1.0' encoding='utf-8'?>
<mapper namespace='a'>
    <select id='selectAll'>
        select * from test where name=:name limit :start,:limit 
    </select>
    <insert id='insert'>
        insert into test(name) values(:name)
    </insert>
    <update id='update'>
        update test set name=:name 
    </update>
    <delete id='delete'>
        delete from test
    </delete>
</mapper>
